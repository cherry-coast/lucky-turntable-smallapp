.lampstand{
  perspective-origin: 1000rpx 60rpx;
  top: -260rpx;
  width: 400rpx;
  height: 400rpx;
  margin: 0 auto;
  text-align: center;
  position: relative;
  perspective: 2000rpx;
  transform-style: preserve-3d;
  transition: all 2s linear 0s;
}
.lampstand .lampstand-item{
  position: absolute;
  width: 200rpx;
  height: 170rpx;
  top:0;
  left: 0;
  border-radius: 8rpx;
}
.lampstand-item:nth-child(1){
  height: 0rpx;
  width: 200rpx;
  margin: 0 auto;
  text-align: center;
  border-left: 80rpx solid transparent;
  border-right: 80rpx solid transparent;
  border-bottom: 400rpx solid #ffb4be;
  transform: translateX(10px);
}
.lampstand-item:nth-child(2){
  top: 218rpx;
  left: 276rpx;
  background-color: #FBDCDD;
  /* border-left-style: solid;
